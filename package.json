{
  "name": "tunde-work",
  "version": "1.0.0",
  "description": "Landing page for Nature",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass --watch --recursive --output css --source-map true --source-map-contents sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font-css.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css"
  },
  "author": "Babatunde",
  "license": "ISC",
  "dependencies": {
    "live-server": "^1.2.1",
    "node-sass": "^5.0.0",
    "node.js": "0.0.1-security",
    "save-dev": "0.0.1-security"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "concat": "^1.0.3",
    "postcss-cli": "^8.3.1",
    "prefixer": "0.0.3"
  }
}
